<br>
<% @recipes.each do |recipe| %>
    <div class="col-md-4">
      <div class="recipe">
        <div class="image-wrapper">


          <%= link_to recipe do %>
              <%= image_tag recipe.image.url(:medium) %>
          <% end %>
        </div>
        <h2> <%= link_to recipe.title, "#" %></h2>
      </div>
    </div>
<% end %>